from itertools import * # Подключаем библиотеку itertools
k = 0 # Создаем счетчик
alf = 'АДЛИН' # Создаем алфавит
for x in product(alf,repeat = 5): # Перибираем все 5-буквенные слова, содержащие буквы из alf
    s = ''.join(x) # Создаем строку
    k += 1 # C каждой новой строкой увеличиваем счетчик на единицу
    if s[2] == 'Н' and s[0] == 'И' and s[-1] == 'И': # Проверяем условие
        print(k) # Если нашлось первое слово, cоответствующее условию, выводим значение счетчика
        break # Останавливаем цикл
