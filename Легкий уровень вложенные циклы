k = 0 # Создаем счетчик
alf = 'АДЛИН' # Создаем алфавит
for s1 in alf:
    for s2 in alf:
        for s3 in alf:    # Каждый из циклов перебирает символы из алфавита.
            for s4 in alf:
                for s5 in alf:
                    s = s1 + s2 + s3 + s4 + s5 # Создаем строку путем сложения символов, перебираемых циклами.
                    k += 1 # C каждой новой строкой увеличиваем счетчик на единицу
                    if s[2] == 'Н' and s[0] == 'И' and s[-1] == 'И': # Проверяем условие
                        print(k) # Если нашлось первое слово, cоответствующее условию, выводим значение счетчика
                        exit() # Останавливаем цикл
