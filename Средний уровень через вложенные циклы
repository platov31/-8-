k = 0 # Создаем счетчик.
alf = '123456789' # Создаем алфавит.
for s1 in alf:
    for s2 in alf:
        for s3 in alf:         # Каждый из циклов перебирает символы из алфавита.
            for s4 in alf:
                for s5 in alf:
                    s = s1 + s2 + s3 + s4 + s5 # Создаем строку путем сложения символов, перебираемых циклами.
                    if s[0] == '8' and s[1] == '9' and s[2] == '9' and int(s[-1]) % 2 == 0:
                        print(k) # Если строка удовлетворяет условию, выводим значение счетчика.
                        exit() # Выходим из циклов, так как нашли номер Пети.
                    k += 1 # До тех пор пока не появилась первая строка удовлетворяющая условию, увеличиваем счетчик на 1.
