from itertools import * # Подключаем библиотеку itertools.
N = 0 # Создаем счетчик слов Майкла.
M = 0 # Создаем счетчик слов Антона.
alf = 'АБВЕОЛ' # Создаем алфавит.
for xn in product(alf,repeat = 6): # Перебираем всевозможные комбинации букв из алфавита, для поиска строк, удовлетворяющих условию Майкла.
    s = ''.join(xn) # Создаем строку.
    s = s.replace('Е','А').replace('О','А') # Заменяем все гласные в строчке на А.
    s = s.replace('В','Б').replace('Л','Б') # Заменяем все согласные в строчке на Б.
    # Делаем замену для того, чтобы было проще проверить чередование гласной с согласной в строке.
    # Таким образом, нам не приедется проверять огромное количество букв.
    if (s[0] == 'А' and s[1] == 'Б' and s[2] == 'А' and s[3] == 'Б' and s[4] == 'А' and s[5] == 'Б') or \
            (s[0] == 'Б' and s[1] == 'А' and s[2] == 'Б' and s[3] == 'А' and s[4] == 'Б' and s[5] == 'А'): # Проверяем строку на чередование гласной с согласной.
        N += 1 # Если строка удовлетворяет условию, увеличиваем счетчик на единицу.
for xm in product(alf,repeat = 6): # Перебираем всевозможные комбинации букв из алфавита, для поиска строк, удовлетворяющих условию Антона.
    s = ''.join(xm) # Создаем строку.
    s = s.replace('В','Б').replace('Л','Б') # Заменяем все согласные в строчке на Б.
    if s.count('Б') == 2 and s[-1] == 'Е': # Проверяем условие Антона.
        M += 1 # Если строка удовлетворяет условию, увеличиваем счетчик на единицу.
print(N + M) # Выводим сумму N и M
