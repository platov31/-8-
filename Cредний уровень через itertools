from itertools import * # Подключаем библиотеку itertools.
k = 0 # Создаем счетчик.
alf = '123456789' # Создаем алфавит.
for x in product(alf, repeat = 5): # Перебираем всевозможные комбинации.
    s = ''.join(x) # Создаем строку.
    if s[0] == '8' and s[1] == '9' and s[2] == '9' and int(s[-1]) % 2 == 0:
        print(k) # Если строка удовлетворяет условию, выводим значение счетчика.
        break # Останавливаем цикл, так как первый номер, соответсвующий условию, является номером Пети.
    k += 1 # До тех пор пока не появилась первая строка удовлетворяющая условию, увеличиваем счетчик на 1.
